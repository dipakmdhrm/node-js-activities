<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Node Tutorial site</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" media="screen" href="/assets/styles/main.css">
</head>
<body>
  <div id="main-wrapper">
    <% include partials/nav.ejs %>
    <h1>Welcome to profile page of <%= username %></h1>
    <p>
      <strong>Age: </strong><%= data.age %><br/>
      <strong>Job: </strong><%= data.job %><br/>
      <strong>Hobbies: </strong><% data.hobbies.forEach(function(item){%>
        <%= item %>
      <%}); %>
    </p>
  </div>
</body>
</html>
